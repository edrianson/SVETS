<Window x:Class="SVETSGUI.ProceduresWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SVETSGUI"
        mc:Ignorable="d"
        Title="ProceduresWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <DataGrid Name="dataGridProcedures" IsReadOnly="True" SelectionChanged="dataGridProcedures_SelectionChanged" Loaded="dataGridProcedures_Loaded"/>
            <TextBox Name="textBoxTreatments" IsReadOnly="True" TextWrapping="WrapWithOverflow"/>
        </StackPanel>
    </Grid>
</Window>
